{
	"name": "singapore-event-scrapper",
	"version": "1.0.0",
	"description": "Scrapes upcoming physical and online events happening in Singapore within the next month from Eventbrite Singapore, and outputs structured event data for further use or analysis.",
	"main": "src/index.ts",
	"scripts": {
		"start": "npm run build && node dist/index.js",
		"build": "tsc",
		"prepare": "husky"
	},
	"author": "Wei Cong",
	"license": "MIT",
	"dependencies": {
		"puppeteer": "^24.10.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@types/node": "^24.0.1",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"globals": "^16.2.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.0"
	},
	"lint-staged": {
		"*": "prettier --write",
		"src/**/*.{ts,tsx}": "eslint --fix"
	}
}
